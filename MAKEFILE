all:
	TASM.EXE /ml VGA.ASM
	TCC.EXE -c -oVGACPP.OBJ VGA.CPP
	TCC.EXE -c -oAPP.OBJ APP.CPP
	TCC.EXE -c -oGOL.OBJ GOL.CPP
	TCC.EXE -c -oRENDERER.OBJ RENDERER.CPP
	TCC.EXE DOSLIFE.CPP APP.OBJ GOL.OBJ VGA.OBJ VGACPP.OBJ RENDERER.OBJ
clean:
	DEL DOSLIFE.EXE
	DEL DOSLIFE.OBJ
	DEL VGA.OBJ
	DEL VGACPP.OBJ
	DEL APP.OBJ
	DEL GOL.OBJ
	DEL RENDERER.OBJ
	
